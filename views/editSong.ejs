<% include partials/header %>
   <%if (err) {%>
        <div style="color:red"><%=err%></div>
        <hr/>
   <%} %>
   <form method="POST" action="/songs">
    Titre:  <input type="text" id="title" name="title" value="<%=song.title%>"><br/>
    Album:  <input type="text" id="album" name="album" value="<%=song.album%>"><br/>
    Artist: <input type="text" id="artist" name="artist" value="<%=song.artist%>"><br/>
    Year:   <input type="text" id="year" name="year" value="<%=song.year%>"><br/>
    BPM:    <input type="text" id="bpm" name="bpm" value="<%=song.bpm%>"><br/>
    <input type="submit"/>
   </form>
<% include partials/footer %>